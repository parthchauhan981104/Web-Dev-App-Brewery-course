<%- include("partials/header") -%>

    <h1>Compose</h1>

    <form action="/compose" method="post">
      <div class="class="form-group"">
        <label>Title</label>
        <input type="text" name="title" class="form-control">
        <label>Post</label>
        <textarea rows="8" cols="50" name="newpost" class="form-control" placeholder="Type you new post here....."></textarea>
      </div>

      <button type="submit" name="button" class="btn btn-primary publish">Publish</button>
    </form>


